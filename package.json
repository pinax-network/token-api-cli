    "type": "git",
    "url": "git+https://github.com/pinax-network/token-api-cli.git"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5",
    "@biomejs/biome": "^2.3.6"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "@pinax/token-api": "^0.1.3"
  }
}